{
    "name": "a11y-webring.club",
    "description": "A webring for digital accessibility practitioners.",
    "version": "1.0.0",
    "homepage": "https://a11y-webring.club/",
    "author": {
        "name": "Eric Bailey",
        "email": "eric.w.bailey@gmail.com",
        "url": "http://ericwbailey.website"
    },
    "scripts": {
      "start": "npx eleventy --serve",
      "build": "eleventy && npm run build:lambda",
      "serve:lambda": "netlify-lambda serve _lambda",
      "build:lambda": "netlify-lambda build _lambda",
      "clean": "rm -rf _site/*"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com:ericwbailey/a11y-webring.club.git"
    },
    "bugs": {
        "url": "https://github.com/ericwbailey/a11y-webring.club/issues"
    },
    "keywords": [
        "a11y",
        "accessibility",
        "webring"
    ],
    "devDependencies": {
        "@11ty/eleventy": "^1.0.2",
        "netlify-lambda": "^1.6.3"
    },
    "dependencies": {
        "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    }
}
